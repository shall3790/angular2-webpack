{
  "name": "ipcdemo",
  "version": "1.11.0",
  "productName": "ipcdemo",
  "author": "Stephen Hall",
  "description": "ipc electron demo app",
  "main": "main.js",
  "scripts": {
    "clean:electron": "rimraf ipcdemo-* && rimraf installer*",
    "build:electron": "npm run clean:electron && electron-packager ./dist ipcdemo --platform=win32 --arch=x64,ia32",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --progress",
    "build:prod": "npm run clean && webpack -p --progress",
    "build2": "npm run clean && tsc",
    "build_prod": "npm run build2 && browserify -s main dist/main.js > dist/bundle.js && npm run minify",
    "electron": "electron main.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "typings install",
    "serve": "webpack-dev-server --inline --progress",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "lite": "lite-server"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "angular2-in-memory-web-api": "0.0.18",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "^3.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "browserify": "^13.0.1",
    "canonical-path": "0.0.2",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^1.2.8",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "jquery": "^3.1.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.2.0",
    "lodash": "^4.11.1",
    "null-loader": "^0.1.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "uglifyjs": "^2.4.10",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",

    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^7.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.3.4"
  }
}
